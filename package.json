{
	"name": "react-pdf-portabletext",
	"description": "Serialize Portable Text to ReactPDF",
	"version": "1.0.1",
	"type": "module",
	"license": "MIT",
	"author": "Johnny Povolny <johnny.povolny@sanity.io>",
	"sideEffects": false,
	"exports": {
		".": {
			"source": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/stats.html",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/portabletext/react-pdf-portabletext.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "pkg-utils build --strict --check --clean",
		"clean": "rimraf dist",
		"test": "vitest --no-file-parallelism --reporter=verbose",
		"format": "prettier --write --cache --ignore-unknown .",
		"lint": "eslint .",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "run-s build lint type-check"
	},
	"browserslist": "extends @sanity/browserslist-config",
	"dependencies": {
		"@fontsource/courier-prime": "^5.2.6",
		"@portabletext/react": "^3.2.1",
		"@portabletext/toolkit": "^2.0.17",
		"@react-pdf/renderer": "^4.3.0",
		"@testing-library/react": "^16.3.0",
		"@types/lodash.isnil": "^4.0.9",
		"flat": "^6.0.1",
		"lodash.isnil": "^4.0.0",
		"lodash.merge": "^4.6.2",
		"lodash.omitby": "^4.6.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.25.0",
		"@portabletext/types": "^2.0.13",
		"@react-pdf/types": "^2.9.0",
		"@sanity/pkg-utils": "^7.9.0",
		"@types/lodash.merge": "^4.6.9",
		"@types/lodash.omitby": "^4.6.9",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.4.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.25.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^8.0.4",
		"pdf-visual-diff": "^0.14.0",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"rimraf": "^6.0.1",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.5",
		"vitest": "^2.1.8"
	},
	"packageManager": "pnpm@10.11.1",
	"publishConfig": {
		"access": "public"
	}
}
